<configs>
    <!-- Bicycle -->
    <set xpath="vehicles/vehicle[@name='vehicleBicycle']/property[@name='velocityMax' and @value='5, 10']/@value">7, 12</set>
    <set xpath="vehicles/vehicle[@name='vehicleBicycle']/property[@name='motorTorqueMinQ' and @value='1000']/@value">1000</set>
    <set xpath="vehicles/vehicle[@name='vehicleBicycle']/property[@name='motorTorqueMaxQ' and @value='1000']/@value">1500</set>

    <!-- Minibike -->
    <set xpath="vehicles/vehicle[@name='vehicleMinibike']/property[@name='velocityMax' and @value='7, 10']/@value">10, 13</set>
    <set xpath="vehicles/vehicle[@name='vehicleMinibike']/property[@name='motorTorqueMinQ' and @value='1000']/@value">1200</set>
    <set xpath="vehicles/vehicle[@name='vehicleMinibike']/property[@name='motorTorqueMaxQ' and @value='2000']/@value">2400</set>

    <!-- Motorcycle -->
    <set xpath="vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax' and @value='9, 14']/@value">15, 22</set>
    <set xpath="vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='motorTorqueMinQ' and @value='2000']/@value">3000</set>
    <set xpath="vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='motorTorqueMaxQ' and @value='3000']/@value">5000</set>

    <!-- 4x4 -->
    <set xpath="vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax' and @value='9, 13']/@value">13, 18</set>
    <set xpath="vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='motorTorqueMinQ' and @value='4000']/@value">3500</set>
    <set xpath="vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='motorTorqueMaxQ' and @value='6000']/@value">4000</set>

    <!-- Gyrocopter -->
    <set xpath="vehicles/vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax' and @value='9, 15']/@value">7, 45</set>
</configs>